{
  "presets": [[
    "env", {
      "targets": {
        "node": "current"
      },
      "modules": false
    }
  ], "es2015", "react", "stage-3"],
  "plugins": [
    "transform-async-to-generator", 
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-runtime",
    "syntax-dynamic-import",
    [
      "import",
      [
        {
          "style": "css",
          "libraryName": "antd"
        }
      ]
    ]
  ]
}