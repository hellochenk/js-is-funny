{
  "presets": [[
    "env", {
      "targets": {
        "node": "current"
      }
    }
  ], "es2015", "react"],
  "plugins": [
    "transform-async-to-generator", 
    "transform-class-properties",
    ["transform-decorators-legacy"],
    ["transform-runtime"],
    [
      "import",
      [
        {
          "style": "css",
          "libraryName": "antd"
        }
      ]
    ]
  ]
}